<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hzlx.mapper.BusinessInfoMapper">

    <select id="selectByUserNameAndPassword" resultType="businessInfo">
        select *
        from t_business_info where user_name = #{userName} and password = #{password};
    </select>
</mapper>